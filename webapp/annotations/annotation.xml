                    </Record>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Label" String="Fecha de la Orden"/>
                        <PropertyValue Property="Value" Path="Orders/RequiredDate"/>
                    </Record>
                </Collection>
            </Annotation>
            <Annotation Term="UI.Facets">
                <Collection>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="InformaciÃ³n de contacto"/>
